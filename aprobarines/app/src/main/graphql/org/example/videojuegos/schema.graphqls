scalar Int

scalar String

type Videojuego{
    id: Int!
    titulo: String!
    descripcion: String!
}

type Personaje{
    id: Int!
    nombre: String!
    descripcion: String!
}

type Mapa{
    id: Int!
    nombre: String!
}

type Query {
    getVideojuegos: [Videojuego!]!
    getPersonajes: [Personaje!]!
    getMapas: [Mapa!]!
}

type Mutation{
    addVideojuego (videojuegoInput : VideojuegoInput): Videojuego
}

input VideojuegoInput {
    id: Int!
    titulo: String!
}


